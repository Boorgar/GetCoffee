<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <FrameLayout android:id="@+id/frameProfile"
        android:layout_height="match_parent"
        android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearRowuser"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_22pxv"
            android:layout_marginEnd="@dimen/_33pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnuser"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_36pxv"
                android:layout_marginTop="@dimen/_53pxv"
                android:layout_weight="0.15" android:layout_width="0dp" android:orientation="vertical">
                <ImageView
                    android:id="@+id/btnUser"
                    android:layout_height="@dimen/_42pxh"
                    android:layout_width="@dimen/_42pxh"
                    android:paddingHorizontal="@dimen/_12pxh"
                    android:paddingVertical="@dimen/_12pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_user" tools:ignore="ContentDescription"/>
                <ImageView
                    android:id="@+id/btnCall"
                    android:layout_height="@dimen/_42pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="@dimen/_42pxh"
                    android:paddingHorizontal="@dimen/_13pxh"
                    android:paddingVertical="@dimen/_13pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_call" tools:ignore="ContentDescription"/>
                <ImageView
                    android:id="@+id/btnCheckmark"
                    android:layout_height="@dimen/_42pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="@dimen/_42pxh"
                    android:paddingHorizontal="@dimen/_12pxh"
                    android:paddingVertical="@dimen/_12pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_checkmark" tools:ignore="ContentDescription"/>
                <ImageView
                    android:id="@+id/btnLocation"
                    android:layout_height="@dimen/_42pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="@dimen/_42pxh"
                    android:paddingHorizontal="@dimen/_12pxh"
                    android:paddingVertical="@dimen/_12pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_location" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearColumntitle"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_weight="0.78" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTitle"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_72pxh"
                    android:layout_marginStart="@dimen/_72pxh"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="Profile"/>

                <TextView
                    android:id="@+id/txtFullname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="Fullname" />

                <TextView
                    android:id="@+id/txtLanguage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="Anderson"/>

                <TextView
                    android:id="@+id/txtPhonenumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="Phone number"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMobileNo"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="0997775555"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmail"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="Email"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtEmailOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="anderson@email.com"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAddress"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="Address"/>
                <TextView android:gravity="start"
                    android:id="@+id/txt3AddersionCou"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:text="3 Addersion Court, Anderson, SC 29621"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnedit"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_47pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_65pxv"
                android:layout_weight="0.07" android:layout_width="0dp" android:orientation="vertical">
                <ImageButton android:id="@+id/imageEdit"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_edit"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
                <ImageButton android:id="@+id/imageEditOne"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_49pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_edit"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
                <ImageButton android:id="@+id/imageEditTwo"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_49pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_edit"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
                <ImageButton android:id="@+id/imageEditThree"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_49pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_edit"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
